package model;

public class Car {
	private int id;  
	private String name;  
	private int price;
	public Car()
	{
		System.out.println("nothing is here.");
	}  
	
	public Car(int id)
	{
		this.id = id;
	}  
	  
	public Car(String name) 
	{  
		this.name = name;
	}
	
	public Car(int id, String name, int price)
	{  
	    this.id = id;  
	    this.name = name;
	    this.price=price;
	}  
	  
	public void show(){  
	    System.out.println("Car id : "+id+", name : "+name+", price : "+price);
	}  
	  

}
